#include<iostream>
#include<vector>
// #include<conio.h>
using namespace std;
void insert(vector<int>& arr,int n,int p){
    arr.pop_back(0);
    int s=arr.size();

    if(s==0){
        arr.push_back(n);
        return;
    }
    for(int i=s;i>p;i--){
        arr[i]=arr[i-1];
    }
    arr[p]=n;
    return;
    
}
int main(){
    vector<int> arr;
    int s;
    cin>>s;
    for(int i=0;i<s;i++){
        int x;
        cin>>x;
        arr.push_back(x);
    }
    int n;
    int p;
    cout<<"element to be insert"<<endl;
    cin>>n;
    cout<<endl;
    cout<<"position"<<endl;
    cin>>p;
    for(int i=0;i<arr.size();i++){
        cout<<arr[i]<<" ";
    }
    insert(arr,n,p);
    cout<<endl;
    for(int i=0;i<arr.size();i++){
        cout<<arr[i]<<" ";
    }

}
